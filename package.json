{
  "name": "@ens-davids/tj-jira-panel",
  "version": "2.9.0",
  "description": "Web Component for integrating TJ with Adobe's Jira",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/tj-jira-panel/tj-jira-panel.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/davids-ensemble/tj-jira-panel.git"
  },
  "files": [
    "dist/",
    "loader/",
    "user-scripts/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec",
    "e2e": "pnpm build && playwright test",
    "generate": "stencil generate",
    "docs": "pnpm build --docs",
    "serve-package": "serve dist -C -p 9898",
    "prerelease": "commit-and-tag-version --prerelease"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.7.4",
    "@stencil/core": "^4.38.3",
    "three-dots": "^0.3.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.19.1",
    "@types/node-localstorage": "^1.3.3",
    "@xmldom/xmldom": "^0.8.11",
    "commit-and-tag-version": "^12.6.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "msw": "2.11.2",
    "node-localstorage": "^3.0.5",
    "serve": "^14.2.5",
    "typescript": "^5.9.3"
  },
  "pnpm": {
    "overrides": {
      "cross-spawn": "7.0.6"
    },
    "onlyBuiltDependencies": [
      "msw"
    ]
  },
  "engines": {
    "node": ">=21.0.0"
  },
  "commit-and-tag-version": {
    "skip": {
      "changelog": true
    }
  },
  "license": "MIT"
}