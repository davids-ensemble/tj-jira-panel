    "@types/node": "^22.7.5",
    "@types/node-localstorage": "^1.3.3",
    "@xmldom/xmldom": "^0.8.10",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "msw": "^2.7.0",
    "node-localstorage": "^3.0.5",
    "serve": "^14.2.4",
    "typescript": "^5.6.3"
