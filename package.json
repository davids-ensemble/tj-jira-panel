{
  "name": "@ens-davids/tj-jira-panel",
  "version": "2.1.0",
  "description": "Web Component for integrating TJ with Adobe's Jira",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
